{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "quotes": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "comma-dangle": "off",
        "no-restricted-imports": "off",
        "no-shadow": "off",
        "implicit-arrow-linebreak": "off",
        "function-paren-newline": "off"
    }
}